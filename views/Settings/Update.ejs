<%- layout("/boilerplate/boilerplate_HomePage") %>
        <div class="create bg-dark">
            <form method="post" action="/profile/Update" class="updateForm needs-validation" novalidate>
                <h1 class="text-white mb-5 font-monospace">Update your Profile</h1>
                <div class="individual">
                    <div class="internal">
                        <label for="i_name" class="form-label">Full Name</label>
                        <input type="text" placeholder="Enter Full Name" id="i_name" name="name" class="form-control" value="<%= result.name %>" required>
                    </div>
                    <div class="internal">
                        <label for="i_username" class="form-label">Username</label>
                        <input type="text" placeholder="Enter Username" id="i_username" name="username" class="form-control" value="<%= result.username %>" required>
                    </div>
                </div>
                <div class="individual">
                    <div class="internal">
                        <label for="i_email" class="form-label">Email</label>
                        <input type="email" placeholder="Enter your Email" id="i_email" name="email" class="form-control" value="<%= result.email %>" required>
                    </div>
                    <div class="internal">
                        <label for="i_phone" class="form-label">Phone Number</label>
                        <input type="text" placeholder="Enter your Phone Number" id="i_phone" name="phone_number" class="form-control" value="<%= result.phone %>" required>
                    </div>
                </div>
                <div class="individual">
                    <button type="submit" class="btn btn-outline-danger mb-2 mt-3">Update Profile</button>
                </div>
            </form>
        </div>
    